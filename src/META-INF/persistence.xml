    <class>Entidades.Finance</class>
    <class>Entidades.HistorialMedico</class>
    <class>Entidades.LevelAcademic</class>
    <class>Entidades.Parents</class>
    <class>Entidades.Persona</class>
    <class>Entidades.State</class>
    <class>Entidades.Student</class>
    <class>Entidades.FamilyComplete</class>
    <class>Entidades.BlackBox</class>
    <class>Entidades.User</class>
    <class>Entidades.UserModule</class>
    <class>Entidades.Module</class>
    <class>Entidades.TypeUser</class>
    <class>Entidades.PropertiesSystem</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/mydb?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="password"/>
    </properties>
  </persistence-unit>
</persistence>
